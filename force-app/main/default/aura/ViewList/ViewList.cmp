<aura:component controller="ContactListController">
    <aura:handler name="init" value="{!this}" action="{!c.callJsFunction}"/>
    <aura:attribute name="listcontact" type="List"></aura:attribute>
    <aura:attribute name="myName" type="String"/>
    
    <table class="slds-table slds-table_cell-buffer slds-table_bordered">
        <thead>
        <tr>
            <th class="" scope="contact">
                <div class ="slds_truncate" title="Contact Name">Name</div>
            </th>
            <th class="" scope="contact">
                <div class ="slds_truncate" title="Contact Id">Id</div>
            </th>
            <th class="" scope="contact">
                <div class ="slds_truncate" title="Contact Email">Email</div>
            </th>
            
            
      </tr>
        
        </thead>
        <tbody>
            <aura:iteration items="{!v.listcontact}" var="con">
                <tr class="ContactListController">
                    <th>
                        <div>
                        {!con.Name}
                        </div>
                        
                        <div>
                        {!con.Id}
                        </div>
                        
                        <div>
                        {!con.Email}
                        </div>
                    </th>
                    <td>
                    
                    <div>{!con.Id}</div>
                    </td>
                </tr>
            </aura:iteration>
         </tbody>
   </table>
</aura:component>